{
  "apiVersion": 2,
  "name": "multivendorx/contact-info",
  "title": "Contact Form",
  "category": "multivendorx-store-shop",
  "icon": "email",
  "description": "Display a contact form with various settings",
  "editorScript": "file:./index.js",
  "script": "file:./index.js",
  "attributes": {
    "hideFromGuests": {
      "type": "boolean",
      "default": false
    },
    "enableGoogleRecaptcha": {
      "type": "boolean",
      "default": false
    },
    "googleRecaptchaType": {
      "type": "string",
      "default": "v2"
    },
    "recaptchaV2Scripts": {
      "type": "string",
      "default": ""
    },
    "recaptchaV3Sitekey": {
      "type": "string",
      "default": ""
    },
    "recaptchaV3Secretkey": {
      "type": "string",
      "default": ""
    }
  }
}